{
  "version": 2,
  "build": {
    "env": {}
  },
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "api/go/email.go", "use": "@now/go" },
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api/email", "dest": "api/go/email/email.go" }, 
    { "src": "/", "status": 301, "headers": { "Location": "/en" } },
    { "src": "/(.*)" }
  ]
}

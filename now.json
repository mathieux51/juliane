{
  "version": 2,
  "build": {
    "env": {}
  },
  "builds": [
    { "src": "api/go/*/*.go", "use": "@now/go" },
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/$1" }, 
    { "src": "/", "status": 301, "headers": { "Location": "/en" } },
    { "src": "/(.*)" }
  ]
}

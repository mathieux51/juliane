{
  "version": 2,
  "build": {
    "env": {}
  },
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "api/go/email.go", "use": "@vercel/go" },
    { "src": "next.config.js", "use": "@vercel/next" }
  ],
  "routes": [
    {
      "src": "/api/email",
      "methods": ["POST"],
      "dest": "api/go/email.go"
    },
    { "src": "/", "status": 301, "headers": { "Location": "/en" } },
    { "src": "/(.*)" }
  ]
}
